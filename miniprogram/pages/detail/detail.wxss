}

.bottom-content {
  width: 100%;
  overflow: hidden;
  position: fixed;
  left: 0;
  bottom: 0;
}

.xiaomi_edit_content {
  width: 100%;
  overflow: hidden;
  background-color: #fff;
  left: 0;
  top: 0;
}

.xiaomi_edit_content_top {
  height: 493rpx;
  color: #fff;
