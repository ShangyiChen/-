.mask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.6);
}

@keyframes first {
  from {
      left: 100%;
  }

  to {
      left: -100%;
  }
}

