}

.detail-comment {
  z-index: 100;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  height: 100%;
  background-color: #fff;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  transition: opacity 300ms ease-out,top 300ms ease-out,height ease-out 150ms;
}

.detail-comment>.detail-comment_stars_ {
  box-shadow: 0 0 15rpx rgba(234,234,234,1);
  border-bottom: 1rpx solid #e5e6e8;
}

.detail-comment>.detail-comment_stars_>.detail-comment_stars {
  width: 300rpx;
  display: flex;
  justify-content: center;
  color: #5d646d;
  margin: 0rpx auto;
}

.detail-comment>.detail-comment_stars_>.detail-comment_stars>view {
  width: 60rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: #5d646d;
  font-size: 46rpx;
  text-align: center;
}

.detail-comment>.detail-comment_title {
  display: block;
  height: 60rpx;
  line-height: 60rpx;
  margin: 30rpx 30rpx -10rpx 30rpx;
  color: #0d141d;
  font-size: 40rpx;
  width: auto;
  -webkit-overflow-scrolling: touch;
}

.detail-comment>.detail-comment_content {
  display: block;
  width: auto;
  flex: 1;
  line-height: 50rpx;
  margin: 30rpx 30rpx 20rpx 30rpx;
  color: #0d141d;
  font-size: 30rpx;
  -webkit-overflow-scrolling: touch;
}

.detail-comment>.detail-comment_info {
  border-top: 1rpx solid #e5e6e8;
  height: 100rpx;
  padding: 30rpx;
  display: flex;
  overflow: hidden;
}

.detail-comment>.detail-comment_info>image {
  display: block;
  height: 100rpx;
  width: calc(100rpx / 4.25 * 3);
}

.detail-comment>.detail-comment_info>view {
  flex: 1;
  overflow: hidden;
  margin-left: 20rpx;
}

.detail-comment>.detail-comment_info>view>view:nth-child(1) {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  color: #49505a;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.detail-comment>.detail-comment_info>view>view:nth-child(2) {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  color: #717881;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.detail-comment>.detail-comment_btns {
  display: flex;
  justify-content: space-between;
  height: 90rpx;
  box-shadow: 0 0 15rpx rgba(234,234,234,1);
  border-top: 1rpx solid #e5e6e8;
}

.detail-comment>.detail-comment_btns>view {
  padding: 0rpx 50rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 46rpx;
  color: #5d646d;
}

.detail-comment-bg {
  z-index: 99;
